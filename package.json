{
  "name": "rboilerplate-node",
  "version": "1.0.0",
  "description": "rboilerplate-node",
  "repository":{
    "git": "https://github.com/dearsaturn/rboilerplate-node"
  },
  "engines": {
    "node": ">=6.6.0",
    "npm": ">=3.10.7"
  },
  "scripts": {
    "start": "npm run build && NODE_ENV=production node dist/server.js",
    "dev-server": "cross-env NODE_ENV=development nodemon lib/server.js --exec babel-node --config ./nodemon.json",
    "test": "jest",
    "clean": "rimraf dist public && mkdirp dist public",
    "build": "npm run clean && babel lib -d dist --presets es2015,stage-0 && npm run build-client",
    "build-client": "cross-env NODE_ENV=production webpack -p --config ./webpack/webpack.prod.config.js",
    "webpack": "webpack --progress --colors"
  },
  "author": "Ricky Sidhu",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-jest": "^15.0.0",
    "babel-polyfill": "^6.13.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-flowtype": "^2.18.2",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "jest": "^15.1.1",
    "nodemon": "^1.10.2"
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "install": "^0.8.1",
    "lodash": "^4.16.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.7.0",
    "node-sass": "^3.10.0",
    "npm": "^3.10.7",
    "react": "^15.3.1",
    "react-addons-transition-group": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1",
    "rimraf": "^2.5.4",
    "sanitizer": "^0.1.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.12.2",
    "whatwg-fetch": "^1.0.0"
  }
}
